{
  "name": "silenzio",
  "version": "0.0.2-alpha.3",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.7.0",
    "next": "^13.5.2",
    "nextjs": "^0.0.3",
    "rollup-plugin-dts": "^6.0.2",
    "sanity": "^3.16.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "description": "Michele Bruno's utility library",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepare": "rm -rf dist && tsc && npm run build:cjs",
    "build:esm": "tsc --module commonjs --outDir dist/esm --project tsconfig.build.json",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --project tsconfig.build.json"
  },
  "exports": {
    ".": "./dist/esm/index.js",
    "./next": "./dist/esm/next/index.js",
    "./sanity": "./dist/esm/sanity/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index.d.ts"
      ],
      "./next": [
        "./dist/esm/next/index.d.ts"
      ],
      "./sanity": [
        "./dist/esm/sanity/index.d.ts"
      ]
    }
  },
  "author": "Michele Bruno",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/michelebruno/silenzio.git"
  }
}
