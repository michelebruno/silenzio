{
  "name": "@silenzio/core",
  "version": "0.0.4",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "repository": "https://github.com/michelebruno/silenzio.git",
  "author": "Michele Bruno",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --coverage",
    "dev": "yarn build:esm --watch",
    "lint": "eslint --fix . && prettier --write .",
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/esm --project tsconfig.build.json",
    "build:cjs": "tsc --outDir dist/cjs --project tsconfig.build.json --module commonjs --moduleResolution node "
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.200",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@silenzio/app-config": "^0.0.4",
    "@silenzio/types": "^0.0.4",
    "lodash": "^4.17.21"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./utils": {
      "types": "./dist/esm/utils.d.ts",
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.js",
      "default": "./dist/cjs/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index.d.ts"
      ],
      "utils": [
        "./dist/esm/utils.d.ts"
      ]
    }
  },
  "gitHead": "0f0c22fd8f9e422e9a3ef0e95ab2804ea033672d"
}
