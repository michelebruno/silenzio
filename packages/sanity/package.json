    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@silenzio/core": "^0.0.4",
    "lodash": "^4.17.21"
  },
  "gitHead": "0f0c22fd8f9e422e9a3ef0e95ab2804ea033672d",
  "peerDependencies": {
    "@silenzio/core": "^0.0.3"
  }
}
