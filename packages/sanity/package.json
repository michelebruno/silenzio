{
  "name": "@silenzio/sanity",
  "version": "0.0.4",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": "https://github.com/michelebruno/silenzio.git",
