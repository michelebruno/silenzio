{
  "name": "@silenzio/sanity",
  "version": "0.0.7-alpha.3",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": "https://github.com/michelebruno/silenzio.git",
  "author": "Michele Bruno",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build && npm run lint && npm run test",
    "test": "jest --coverage",
    "dev": "npm run build:cjs -- --watch",
    "lint": "eslint --fix ../next && prettier --write .",
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/esm --project tsconfig.build.json",
    "build:cjs": "tsc --outDir dist/cjs --project tsconfig.build.json --module commonjs --moduleResolution node "
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@silenzio/core": "^0.0.8-alpha.0",
    "lodash": "^4.17.21"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index.d.ts"
      ]
    }
  },
  "gitHead": "a70457f7bcb0c50eafad109a2ea7409bcb9a672b",
  "peerDependencies": {
    "@sanity/types": "^3.23.4",
    "sanity": "^3.23.4"
  }
}
